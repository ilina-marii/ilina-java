public class Main {

        public static void main(String[] args) {

            byte[] arr1 = new byte[10];

            for (int i=0; i<10; i++) {
                arr1[i] = (byte) (i + 1);
                if (arr1[i] % 2 == 0) {
                System.out.print(arr1[i] + " ");
                };
            }

        }
}
